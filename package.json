{
  "name": "my-profile",
  "version": "1.0.0",
  "description": "MyProfile Backend - A secure and scalable authentication system",
  "main": "dist/server.js",
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 node dist/server.js",
    "dev": "NODE_NO_WARNINGS=1 nodemon src/server.ts",
    "build": "tsc && cp -r src/templates dist/templates && cp -r public dist/public",
    "postinstall": "npm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "validate-logs": "ts-node src/tests/validate-logging.ts",
    "test-tracking": "ts-node src/tests/test-tracking.ts",
    "verify-tracking": "npm run validate-logs && npm run test-tracking",
    "license:generate-secret": "ts-node scripts/generate-company-secret.ts",
    "license:setup-admin": "ts-node scripts/setup-admin-license.ts",
    "license:validate": "ts-node scripts/validate-license.ts",
    "license:list": "ts-node scripts/list-licensed-employees.ts",
    "license:generate": "ts-node scripts/generate-hardware-employee-license.ts",
    "license:info": "ts-node scripts/validate-license.ts",
    "prestart": "npm run license:validate",
    "predev": "npm run license:validate"
  },
  "keywords": [
    "authentication",
    "typescript",
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express-session": "^1.18.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bowser": "^2.11.0",
    "canvas": "^3.0.0",
    "cloudinary": "^2.6.0",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "detect-browser": "^5.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "fingerprintjs2": "^2.1.4",
    "geoip-lite": "^1.4.10",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "maxmind": "^4.3.24",
    "memcached": "^2.2.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.16",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "puppeteer-core": "^18.1.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "spdy": "^4.0.2",
    "speakeasy": "^2.0.0",
    "stripe": "^18.0.0",
    "twilio": "^5.5.1",
    "ua-parser-js": "^2.0.2",
    "uuid": "^11.0.5",
    "whatsapp-web.js": "^1.26.0",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.2",
    "@types/geoip-lite": "^1.4.4",
    "@types/handlebars": "^4.0.40",
    "@types/http-errors": "^2.0.4",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-linkedin-oauth2": "^1.5.6",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/sharp": "^0.32.0",
    "@types/spdy": "^3.4.9",
    "@types/speakeasy": "^2.0.10",
    "@types/twilio": "^3.19.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
